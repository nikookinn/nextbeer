{
  "name": "nextbeer-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:production": "npm run type-check && npm run lint && vite build --mode production",
    "build:staging": "npm run type-check && vite build --mode staging",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "analyze": "vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist node_modules/.vite",
    "type-check": "tsc --noEmit",
    "production-test": "node scripts/production-test.js",
    "sonar": "sonar-scanner",
    "sonar:install": "npm install -g sonarqube-scanner",
    "quality:check": "npm run lint && npm run type-check && npm run sonar"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.0",
    "@mui/x-date-pickers": "^6.18.0",
    "@radix-ui/colors": "^3.0.0",
    "@reduxjs/toolkit": "^2.0.0",
    "@types/leaflet": "^1.9.20",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^12.23.22",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-leaflet": "^4.2.1",
    "react-redux": "^9.0.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/aos": "^3.0.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-refresh": "^0.4.0",
    "globals": "^16.4.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "typescript": "^5.2.0",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-compression": "^0.5.1"
  }
}
